/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=a,out=nota);
	Not(in=b,out=notb);
	And(a=a,b=notb,out=c);
	And(a=b,b=nota,out=d);
	Or(a=c,b=d,out=out);
}